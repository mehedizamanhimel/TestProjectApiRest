<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->
        <theme>standard, dark</theme>

        <!-- document encoding -->
        <encoding>UTF-8</encoding>

        <!-- protocol for script and stylesheets -->
        <protocol>https</protocol>

        <!-- title of the document -->
        <documentTitle>FreeNow WebTest</documentTitle>

        <!-- report name - displayed at top-nav -->
        <reportName>Test report for Web Automation Free Now</reportName>

        <!-- report headline - displayed at top-nav, after reportHeadline -->
        <reportHeadline>Test report for Web Automation Free Now details</reportHeadline>

        <!-- global date format override -->
        <!-- defaults to yyyy-MM-dd -->
        <dateFormat>yyyy-MM-dd</dateFormat>

        <!-- global time format override -->
        <!-- defaults to HH:mm:ss -->
        <timeFormat>HH:mm:ss</timeFormat>

        <!-- custom javascript -->
        <scripts>
            <![CDATA[
        $(document).ready(function() {

        });
      ]]>
        </scripts>

        <!-- custom styles -->
        <styles>
            <![CDATA[

      ]]>
        </styles>
    </configuration>
</extentreports>